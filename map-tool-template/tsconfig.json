{
    "compilerOptions": {
        "module": "amd",
        "sourceMap": true,
        "jsx": "react",
        "jsxFactory": "tsx",
        "target": "es5",
		"moduleResolution": "node",
        "experimentalDecorators": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": ["dom", "es2015.promise", "es5"]
    },
    "include": [
        "template.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}